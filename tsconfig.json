{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/modal.jsx",
    "app/_layout.jsx",
    "app/(tabs)/_layout.tsx",
    "app/(tabs)/explore.jsx",
    "app/(tabs)/index.tsx.jsx",
    "nativewind-env.d.ts"
  ]
}